{
  "name": "itech-world/sulu-grapesjs-bundle",
  "description": "SuluGrapesJsBundle extends the Sulu CMS to offer GrapeJS editor integration in Sulu Admin for content editing",
  "type": "sulu-bundle",
  "license": "MIT",
  "version": "0.1.3",
  "keywords": [
    "sulu",
    "symfony",
    "symfony-ux",
    "cms",
    "content-management-system",
    "php",
    "grapesjs",
    "editor"
  ],
  "authors": [
      {
      "name": "Steeven THOMAS",
      "homepage": "https://github.com/steeven-th"
      }
  ],
  "require": {
      "php": "^8.2",
      "sulu/sulu": "^2.6",
      "symfony/stimulus-bundle": "^2.17"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/debug-bundle": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/error-handler": "^7.3",
    "symfony/phpunit-bridge": "^7.3",
    "symfony/var-dumper": "^7.3",
    "symfony/web-profiler-bundle": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "ItechWorld\\SuluGrapesJsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ItechWorld\\SuluGrapesJsBundle\\": "src/tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "phpunit": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../../assets/admin/node_modules",
      "options": {
        "symlink": true
      }
    }
  ]
}
